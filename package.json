{
  "private": true,
  "name": "leoavelino",
  "description": "Leonardo Avelino Blog",
  "license": "MIT",
  "scripts": {
    "build": "tailwindcss -i ./src/styles/index.css -o ./src/styles/dist.css -m && remix build;",
    "css": "tailwindcss -i ./src/styles/index.css -o ./src/styles/dist.css --watch",
    "database:local": "docker-compose up",
    "start": "remix-serve api/_build",
    "dev": "concurrently 'yarn css' 'dotenv -e .env.development remix dev'",
    "postinstall": "remix setup node",
    "prisma:studio": "dotenv -e .env.development prisma studio",
    "prisma:deploy": "dotenv -e .env.development prisma migrate deploy",
    "prisma:migrate": "dotenv -e .env.development prisma migrate dev",
    "prisma:reset": "dotenv -e .env.development prisma migrate reset",
    "prisma:pull": "dotenv -e .env.development prisma db pull",
    "prisma:push": "dotenv -e .env.development prisma db push",
    "prettier:check": "prettier --check 'src/**/*.(tsx|ts|json)'",
    "prettier:write": "prettier --write 'src/**/*.(tsx|ts|json)'",
    "magic:export:components": "node ./scripts/export-components"
  },
  "dependencies": {
    "@prisma/client": "^4.4.0",
    "@remix-run/react": "^1.7.2",
    "@remix-run/serve": "^1.7.2",
    "@remix-run/vercel": "^1.7.2",
    "@tippyjs/react": "^4.2.6",
    "classnames": "^2.3.2",
    "dotenv": "^16.0.2",
    "i18next": "^21.9.2",
    "i18next-browser-languagedetector": "^6.1.5",
    "i18next-fs-backend": "^1.1.5",
    "i18next-http-backend": "^1.4.4",
    "postcss": "^8.4.16",
    "prisma": "^4.4.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-focus-lock": "^2.9.1",
    "react-i18next": "^11.18.6",
    "react-loading-skeleton": "^3.1.0",
    "react-syntax-highlighter": "^15.5.0",
    "react-toastify": "^9.0.8",
    "rehype-document": "^6.1.0",
    "rehype-parse": "^8.0.4",
    "rehype-react": "^7.1.1",
    "rehype-stringify": "^9.0.3",
    "remark-breaks": "^3.0.2",
    "remark-gfm": "^3.0.1",
    "remark-parse": "^10.0.1",
    "remark-rehype": "^10.1.0",
    "remix": "^1.7.2",
    "remix-i18next": "^4.1.1",
    "unified": "^10.1.2"
  },
  "devDependencies": {
    "@remix-run/dev": "^1.7.2",
    "@types/i18next-fs-backend": "^1.1.2",
    "@types/react": "^18.0.21",
    "@types/react-dom": "^18.0.6",
    "@types/react-syntax-highlighter": "^15.5.5",
    "concurrently": "^7.4.0",
    "dotenv-cli": "^6.0.0",
    "prettier": "^2.7.1",
    "tailwindcss": "^3.1.8",
    "typescript": "^4.8.4",
    "yarn-upgrade-all": "^0.7.1"
  },
  "engines": {
    "node": ">=14"
  },
  "sideEffects": false
}
