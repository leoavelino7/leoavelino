{
  "private": true,
  "name": "leoavelino",
  "description": "Leo Avelino Blog",
  "license": "MIT",
  "scripts": {
    "build": "tailwindcss -i ./src/styles/index.css -o ./src/styles/dist.css -m && remix build;",
    "css": "tailwindcss -i ./src/styles/index.css -o ./src/styles/dist.css --watch",
    "database:local": "docker-compose up",
    "start": "remix-serve api/_build",
    "dev": "concurrently 'yarn css' 'dotenv -e .env.development remix dev'",
    "postinstall": "remix setup node",
    "prisma:studio": "dotenv -e .env.development prisma studio",
    "prisma:deploy": "prisma migrate deploy",
    "prisma:migrate": "prisma migrate dev",
    "prisma:reset": "prisma migrate reset",
    "prisma:pull": "prisma db pull",
    "prisma:push": "prisma db push",
    "prettier:check": "prettier --check 'src/**/*.(tsx|ts|json)'",
    "prettier:write": "prettier --write 'src/**/*.(tsx|ts|json)'"
  },
  "dependencies": {
    "@prisma/client": "4.0.0",
    "@remix-run/react": "1.6.2",
    "@remix-run/serve": "1.6.2",
    "@remix-run/vercel": "1.6.2",
    "@tippyjs/react": "4.2.6",
    "classnames": "2.3.1",
    "dotenv": "16.0.1",
    "postcss": "8.4.14",
    "prisma": "4.0.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-syntax-highlighter": "15.5.0",
    "react-toastify": "9.0.7",
    "rehype-document": "6.0.1",
    "rehype-parse": "8.0.4",
    "rehype-react": "7.1.1",
    "rehype-stringify": "9.0.3",
    "remark-breaks": "3.0.2",
    "remark-gfm": "3.0.1",
    "remark-parse": "10.0.1",
    "remark-rehype": "10.1.0",
    "remix": "1.6.5",
    "unified": "10.1.2"
  },
  "devDependencies": {
    "@remix-run/dev": "1.6.2",
    "@types/react": "18.0.14",
    "@types/react-dom": "18.0.5",
    "@types/react-syntax-highlighter": "15.5.2",
    "concurrently": "7.2.2",
    "dotenv-cli": "6.0.0",
    "prettier": "2.7.1",
    "tailwindcss": "3.1.4",
    "typescript": "4.7.4"
  },
  "engines": {
    "node": ">=14"
  },
  "sideEffects": false
}
